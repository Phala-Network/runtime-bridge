  "scripts": {
    "watch-ts": "tsc -w",
    "proto:build": "pbjs -t static-module --no-beautify -p ./src/vendor/proto -o ./src/message/proto.generated.js -w es6 ./src/vendor/proto/message.proto && sed -i \"s/import \\* as $protobuf/import $protobuf/g\" ./src/message/proto.generated.js",
    "proto:build_prpc": "pbjs -t static-module --no-beautify -p ./src/vendor/prpc-proto -o ./src/utils/prpc/proto.generated.js -w es6 ./src/vendor/prpc-proto/*.proto && sed -i \"s/import \\* as $protobuf/import $protobuf/g\" ./src/utils/prpc/proto.generated.js",
    "proto:darwin:build": "pbjs -t static-module --no-beautify -p ./src/vendor/proto -o ./src/message/proto.generated.js -w es6 ./src/vendor/proto/message.proto && gsed -i \"s/import \\* as $protobuf/import $protobuf/g\" ./src/message/proto.generated.js",
    "proto:darwin:build_prpc": "pbjs -t static-module --no-beautify -p ./src/vendor/prpc-proto -o ./src/utils/prpc/proto.generated.js -w es6 ./src/vendor/prpc-proto/*.proto && gsed -i \"s/import \\* as $protobuf/import $protobuf/g\" ./src/utils/prpc/proto.generated.js",
    "start_module": "node --trace-warnings --experimental-json-modules --es-module-specifier-resolution=node --harmony-top-level-await dist/index",
    "dump_workers": "node --trace-warnings --experimental-json-modules dist/scripts/dump_workers",
    "import_workers": "node --trace-warnings --experimental-json-modules dist/scripts/import_workers",
    "debug_process_para_block": "node --inspect-brk=0.0.0.0:9229 --trace-warnings --experimental-json-modules dist/scripts/debug_process_para_block",
    "generate_chain_types": "yarn polkadot-types-from-chain --endpoint wss://khala-api.phala.network/ws --output src/utils/phala-types"
  }
